{
  "name": "@jsx-email/all",
  "version": "2.2.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "A package containing all available JSX email components ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/components"
  },
  "homepage": "https://jsx.email/docs/components/all",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "email"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@jsx-email/body": "workspace:*",
    "@jsx-email/button": "workspace:*",
    "@jsx-email/column": "workspace:*",
    "@jsx-email/container": "workspace:*",
    "@jsx-email/font": "workspace:*",
    "@jsx-email/head": "workspace:*",
    "@jsx-email/heading": "workspace:*",
    "@jsx-email/hr": "workspace:*",
    "@jsx-email/html": "workspace:*",
    "@jsx-email/img": "workspace:*",
    "@jsx-email/link": "workspace:*",
    "@jsx-email/markdown": "workspace:*",
    "@jsx-email/preview": "workspace:*",
    "@jsx-email/render": "workspace:*",
    "@jsx-email/row": "workspace:*",
    "@jsx-email/section": "workspace:*",
    "@jsx-email/tailwind": "workspace:*",
    "@jsx-email/text": "workspace:*"
  },
  "types": "dist/index.d.mts",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shellscape"
  },
  "sideEffects": false
}
