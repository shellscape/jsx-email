  "publishConfig": {
    "access": "public"
  },
  "description": "A JSX email component that converts markdown input to valid jsx-email syntax.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
