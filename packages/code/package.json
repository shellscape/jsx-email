{
  "name": "@jsx-email/code",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A JSX email component which displays a syntax-highlighted code block using https://shiki.matsu.io/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/code"
  },
  "homepage": "https://jsx.email/docs/components/code",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "email"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@jsx-email/render": "workspace:*",
    "p-memoize": "4.0.4",
    "shikiji": "^0.6.8"
  },
  "types": "dist/index.d.mts",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shellscape"
  },
  "sideEffects": false
}
