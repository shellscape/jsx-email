  "peerDependencies": {
    "jsx-email": "workspace:^"
  },
  "dependencies": {
    "pretty": "2.0.0"
  },
  "devDependencies": {
    "@types/pretty": "^2.0.1"
  },
  "types": "./dist/commonjs/index.d.ts",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shellscape"
  },
  "sideEffects": false,
  "tshy": {
