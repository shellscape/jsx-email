{
  "name": "@jsx-email/plugin-pretty",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A prettifying plugin for jsx-email",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/plugin-pretty"
  },
  "author": "Andrew Powell <andrew@shellscape.org>",
  "homepage": "https://jsx.email/",
  "main": "./dist/commonjs/index.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
