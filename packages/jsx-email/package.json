{
  "name": "jsx-email",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Render JSX email components to HTML email",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/render"
  },
  "homepage": "https://jsx.email/docs/core/render",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "email"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.0.2",
    "hash-it": "^6.0.0",
    "html-to-text": "9.0.5",
    "md-to-react-email": "5.0.0",
    "p-memoize": "4.0.4",
    "pretty": "2.0.0",
    "rehype": "^13.0.1",
    "rehype-preset-minify": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "shikiji": "^0.6.8",
    "twind": "^0.16.19",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/html-minifier-terser": "^7.0.0",
    "@types/html-to-text": "^9.0.2",
    "@types/object-hash": "^3.0.6",
    "@types/pretty": "^2.0.1",
    "hast": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "types": "dist/index.d.mts",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shellscape"
  },
  "sideEffects": false
}
