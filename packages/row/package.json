{
  "name": "@jsx-email/row",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A JSX email layout component for displaying rows that separate content areas horizontally in your email.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/row"
  },
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "test": "vi"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "email"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false
}
