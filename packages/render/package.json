{
  "name": "@jsx-email/render",
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Render JSX email components to HTML email",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/render"
  },
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "react",
    "email"
  ],
  "dependencies": {
    "html-to-text": "9.0.5",
    "pretty": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@jsx-email/all": "workspace:*",
    "@types/html-to-text": "^9.0.2",
    "@types/pretty": "^2.0.1"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false
}
