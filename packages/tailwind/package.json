{
  "name": "@jsx-email/tailwind",
  "version": "3.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "A JSX email component which enables using Tailwind CSS to style emails",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shellscape/jsx-email.git",
    "directory": "packages/tailwind"
  },
  "homepage": "https://jsx.email/docs/components/tailwind",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "jsx",
    "email",
    "react",
    "tailwind"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@jsx-email/render": "workspace:*",
    "cheerio": "1.0.0-rc.12",
    "twind": "^0.16.19"
  },
  "devDependencies": {
    "@jsx-email/head": "workspace:*",
    "@jsx-email/hr": "workspace:*",
    "@jsx-email/html": "workspace:*",
    "react": "18.2.0"
  },
  "types": "dist/index.d.mts",
  "bump": "1",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shellscape"
  },
  "sideEffects": false
}
