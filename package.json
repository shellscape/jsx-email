    "@jsx-email/html": "^1.0.0",
    "@jsx-email/img": "^1.0.0",
    "@jsx-email/link": "^1.0.0",
    "@jsx-email/markdown": "^2.0.0",
    "@jsx-email/preview": "^1.0.0",
    "@jsx-email/row": "^1.0.0",
    "@jsx-email/section": "^1.0.0",
    "@jsx-email/tailwind": "^2.0.0",
    "@jsx-email/text": "^1.0.0",
