    "((.github/**/*)|(README|CHANGELOG)|(**/(README|CHANGELOG))).md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.7.1",
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.22",
      "@types/react-dom": "18.2.7",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-router-dom": "6.16.0"
    }
