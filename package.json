{
  "name": "repo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "./shared/bootstrap.sh",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@dot/versioner": "^0.3.4",
    "@moonrepo/cli": "^1.17.3",
    "@swc/core": "^1.3.91",
    "@swc/helpers": "^0.5.2",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "create-jsx-email": "workspace:*",
    "csstype": "3.1.2",
    "eslint-config-shellscape": "^6.0.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "execa": "^9.4.1",
    "gh-pages": "^6.0.0",
    "happy-dom": "^12.2.1",
    "husky": "^8.0.3",
    "jsx-email": "workspace:*",
    "lint-staged": "14.0.1",
    "strip-ansi": "^7.1.0",
    "ts-node": "10.9.1",
    "tshy": "^1.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --cache"
    ],
    "**/(package|tsconfig(.*)?).json": [
      "prettier --write"
    ],
    "(pnpm-workspace|.github/**/*).{yml,yaml}": [
      "prettier --write"
    ],
    "*package.json": [
      "prettier --write --plugin=prettier-plugin-package"
    ],
    "((.github/**/*)|(docs/**/*)|(web/**/*.md)|(README|CHANGELOG)|(**/(README|CHANGELOG))).md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.1.1",
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.22",
      "@types/react-dom": "18.2.7",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-router-dom": "6.16.0"
    }
  }
}
